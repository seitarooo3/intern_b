
<% if @user.present? %>
  <% @object = @user %>
<% elsif @branch.present? %>
  <% @object = @branch %>
<% end %>

<% if @object.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
      The form contains <%= pluralize(@object.errors.count, "error") %>.
    </div>
    <ul>
    <% @object.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>